{
  "name": "uip-weatherapplication",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "express": "^4.19.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "node build/server.js",
    "build": "webpack --mode production && npm run build:css",
    "build:css": "tailwindcss build ./src/globals.css -o ./build/public/styles.css",
    "build:client": "react-scripts build",
    "dev:client": "react-scripts start",
    "dev:server": "nodemon --exec babel-node src/server.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/node": "^7.25.0",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.24.7",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^7.1.2",
    "daisyui": "^4.12.10",
    "mini-css-extract-plugin": "^2.9.1",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.41",
    "postcss-loader": "^8.1.1",
    "tailwindcss": "^3.4.10",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
